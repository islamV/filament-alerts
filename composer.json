{
    "name": "tomatophp/filament-alerts",
    "type": "library",
    "description": "Send notification to users using notification templates and multi notification channels, supports Filament Native Notification Service and FCM integration",
    "keywords": [
        "php",
        "laravel",
        "notifications",
        "filament",
        "alerts",
        "TALL stack",
        "email",
        "database"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentAlerts\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentAlerts\\FilamentAlertsServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^4.0",
        "filament/notifications": "^4.0",
          "tomatophp/filament-settings-hub": "^4.0"
    },
    "require-dev": {
        "livewire/livewire": "^2.10|^3.0",
        "orchestra/testbench": "^9.5",
        "pestphp/pest": "^2.36"
    },
    "version": "2.2.3",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
